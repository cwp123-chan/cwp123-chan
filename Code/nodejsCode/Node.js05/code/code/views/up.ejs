<div class="container">
    <div class="row">
        <form method="post" action="/up" enctype="multipart/form-data">
            <div class="form-group">
                <label class="col-md-6">选择文件夹: </label>
                <div class="col-md-6">
                    <select class="form-control" name="wjj">
                        <% for (var i = 0; i < pics.length; i++) { %>
                            <option><%= pics[i] %></option>
                        <% } %>
                    </select>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-6"> 上传图片: </label>
                <div class="col-md-6">
                    <input type="file" name="myfile">
                </div>

            </div>
            <br>
            <br>
            <br>
            <button type="submit" class="btn btn-primary btn-lg">开始上传</button>
        </form>
    </div>
</div>


<script src="/js/jquery.min.js"></script>
